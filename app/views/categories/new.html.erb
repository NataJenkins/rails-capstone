<h1>Categories#new</h1>
<div>
  <%= form_with model: @category, class: 'form' do |f| %>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name, class: 'input' %>
    </div>
    <div>
      <%= f.label :priority %>
      <%= f.select :priority, [1, 2, 3, 4, 5], class: 'input' %>
    </div>
    <%= f.submit %>
  <% end %>
</div>